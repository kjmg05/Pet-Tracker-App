{
  "expo": {
    "name": "taskily-app",
    "slug": "taskily-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Taskily-logo.png",
    "splash": {
      "image": "./assets/Taskily-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FBFBFB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Taskily-logo-square.png",
        "backgroundColor": "#FBFBFB"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
